{
  "name": "@barocss/text-analyzer",
  "version": "0.1.0",
  "description": "Smart text change analyzer with LCP/LCS algorithm and selection bias",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "text-analysis",
    "lcp-lcs",
    "text-diff",
    "unicode",
    "selection-bias"
  ],
  "author": "Barocss Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.19.24",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vitest": "^1.6.1"
  },
  "publishConfig": {
    "access": "public",
    "files": [
      "src"
    ],
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    }
  }
}
