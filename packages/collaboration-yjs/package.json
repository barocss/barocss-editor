{
  "name": "@barocss/collaboration-yjs",
  "version": "0.1.0",
  "description": "Yjs adapter for Barocss Editor collaborative editing",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@barocss/collaboration": "workspace:*",
    "@barocss/datastore": "workspace:*"
  },
  "peerDependencies": {
    "yjs": "^13.6.0"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    },
    "files": [
      "dist"
    ]
  },
  "keywords": [
    "collaboration",
    "yjs",
    "crdt",
    "realtime",
    "editor"
  ],
  "author": "Barocss Team",
  "license": "MIT"
}

