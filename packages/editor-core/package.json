{
  "name": "@barocss/editor-core",
  "version": "1.0.0",
  "description": "Core editor logic that orchestrates schema, model, and renderers",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@barocss/datastore": "workspace:*",
    "@barocss/model": "workspace:*",
    "@barocss/renderer-dom": "workspace:*",
    "@barocss/shared": "workspace:*",
    "@barocss/schema": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.1"
  },
  "keywords": [
    "editor",
    "core",
    "orchestration",
    "document",
    "wysiwyg"
  ],
  "author": "Barocss Team",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    },
    "files": [
      "dist"
    ]
  }
}
