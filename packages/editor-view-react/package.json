{
  "name": "@barocss/editor-view-react",
  "version": "1.0.0",
  "description": "React-based EditorView with hooks and components over schema/model/datastore",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@barocss/datastore": "workspace:*",
    "@barocss/model": "workspace:*",
    "@barocss/renderer-react": "workspace:*",
    "@barocss/schema": "workspace:*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^3.9.1"
  },
  "keywords": [
    "editor",
    "view",
    "react",
    "wysiwyg"
  ],
  "author": "Barocss Team",
  "license": "MIT"
}
