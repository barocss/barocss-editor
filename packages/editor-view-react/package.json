{
  "name": "@barocss/editor-view-react",
  "version": "0.1.0",
  "description": "React view layer for Barocss Editor (renderer-react + Editor)",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@barocss/dsl": "workspace:*",
    "@barocss/editor-core": "workspace:*",
    "@barocss/renderer-react": "workspace:*",
    "@barocss/shared": "workspace:*",
    "@barocss/text-analyzer": "workspace:*",
    "@barocss/text-run-index": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^3.9.1"
  },
  "keywords": ["editor", "view", "react", "wysiwyg"],
  "author": "Barocss Team",
  "license": "MIT"
}
