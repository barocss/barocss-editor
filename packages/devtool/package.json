{
  "name": "@barocss/devtool",
  "version": "0.1.0",
  "description": "Development tools for Barocss Editor - visualize editor structure and events",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    },
    "files": [
      "dist"
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@barocss/editor-core": "workspace:*",
    "@barocss/model": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.19.24",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^4.5.4"
  },
  "keywords": [
    "devtool",
    "editor",
    "debug",
    "development"
  ],
  "author": "Barocss Team",
  "license": "MIT"
}

