{
  "name": "@barocss/renderer-dom",
  "version": "1.0.0",
  "description": "Renderer DSL for declarative DOM rendering",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    },
    "files": [
      "dist"
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@barocss/dsl": "workspace:*",
    "@barocss/text-run-index": "workspace:*"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "jsdom": "^24.1.3",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^4.0.7"
  },
  "keywords": [
    "renderer",
    "dom",
    "dsl",
    "template",
    "editor"
  ],
  "author": "Barocss Team",
  "license": "MIT"
}
