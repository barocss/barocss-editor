{
  "name": "@barocss/converter",
  "version": "1.0.0",
  "description": "Document format converter for Barocss Editor",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@barocss/datastore": "workspace:*"
  },
  "devDependencies": {
    "jsdom": "^24.1.3",
    "latex-utensils": "^6.2.0",
    "markdown-it": "^14.1.0",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.1"
  },
  "keywords": [
    "converter",
    "html",
    "markdown",
    "latex",
    "format"
  ],
  "author": "Barocss Team",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    },
    "files": [
      "dist"
    ]
  }
}
