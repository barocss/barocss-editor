{
  "name": "@barocss/shared",
  "version": "1.0.0",
  "description": "Shared utilities and constants for Barocss Editor",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "devDependencies": {
    "jsdom": "^24.1.3",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.1"
  },
  "keywords": [
    "shared",
    "utilities",
    "constants",
    "platform"
  ],
  "author": "Barocss Team",
  "license": "MIT"
}

